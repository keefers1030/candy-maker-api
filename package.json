    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:migrate": "env-cmd ./node_modules/.bin/env-cmd/node_modules/.bin/sequelize db: migrate --env development --config ./config/sequelize.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyschachter/candy-maker-api.git"
  },
